!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true

!urxvt
urxvt*geometry:		     86x42
urxvt*depth:		       32
urxvt*foreground:	     #ffffff
urxvt*background:      rgba:0000/0000/1111/dddd
urxvt.font:            xft:Inconsolata for Powerline:size=15
urxvt*urlLauncher:     firefox
urxvt*scrollBar:		   false
urxvt*scrollBar_right: false
urxvt*scrollstyle:		 plain
urxvt*scrollColor:		 #323232
urxvt*cursorColor:		 #eeeeee

!*background:#161616
!black
*color0:    #000000
*color8:    #161616
!red
*color1:    #ee3216
*color9:    #ff6432
!green
*color2:    #96ca2c
*color10:   #c8d686
!yellow
*color3:    #89725b
*color11:   #e7e1b7
!blue
*color4:    #4192d9
*color12:   #a5cffc
!magenta
*color5:    #9900ff
*color13:   #cc99ff
!cyan
*color6:    #424242
*color14:   #868686
!white
*color7:    #dddddd
*color15:   #ffffff
